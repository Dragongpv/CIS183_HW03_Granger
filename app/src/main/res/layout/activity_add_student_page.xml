<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddStudentPage">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".StudentDetailsPage"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="-16dp">

        <EditText
            android:id="@+id/et_v_addStudent_lname"
            android:layout_width="301dp"
            android:layout_height="49dp"
            android:ems="10"
            android:inputType="text"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="140dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Username:"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="16dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="First Name:"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="76dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Last Name:"
            tools:layout_editor_absoluteX="13dp"
            tools:layout_editor_absoluteY="156dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Age:"
            tools:layout_editor_absoluteX="29dp"
            tools:layout_editor_absoluteY="297dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="GPA:"
            tools:layout_editor_absoluteX="19dp"
            tools:layout_editor_absoluteY="366dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Major:"
            tools:layout_editor_absoluteX="19dp"
            tools:layout_editor_absoluteY="433dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email:"
            tools:layout_editor_absoluteX="19dp"
            tools:layout_editor_absoluteY="230dp" />

        <EditText
            android:id="@+id/et_v_addStudent_fname"
            android:layout_width="301dp"
            android:layout_height="49dp"
            android:ems="10"
            android:inputType="text"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="78dp" />

        <EditText
            android:id="@+id/et_v_addStudent_username"
            android:layout_width="301dp"
            android:layout_height="49dp"
            android:ems="10"
            android:inputType="text"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="16dp" />

        <EditText
            android:id="@+id/et_v_addStudent_age"
            android:layout_width="301dp"
            android:layout_height="49dp"
            android:ems="10"
            android:inputType="text"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="282dp" />

        <EditText
            android:id="@+id/et_v_addStudent_gpa"
            android:layout_width="301dp"
            android:layout_height="49dp"
            android:ems="10"
            android:inputType="text"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="351dp" />

        <EditText
            android:id="@+id/et_v_addStudent_email"
            android:layout_width="301dp"
            android:layout_height="49dp"
            android:ems="10"
            android:inputType="text"
            tools:layout_editor_absoluteX="94dp"
            tools:layout_editor_absoluteY="214dp" />

        <Button
            android:id="@+id/btn_v_details_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Back"
            tools:layout_editor_absoluteX="32dp"
            tools:layout_editor_absoluteY="658dp" />

        <Button
            android:id="@+id/btn_v_details_update"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Update"
            tools:layout_editor_absoluteX="305dp"
            tools:layout_editor_absoluteY="653dp" />

        <Spinner
            android:id="@+id/sp_v_addStudent_major"
            android:layout_width="311dp"
            android:layout_height="28dp"
            android:layout_marginEnd="1dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView11"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView11" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>