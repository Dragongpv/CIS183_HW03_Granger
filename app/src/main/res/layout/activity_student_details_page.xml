<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StudentDetailsPage">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="6dp"
        android:text="ID:"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="@+id/textView6" />

    <TextView
        android:id="@+id/tv_v_details_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="6dp"
        android:text="id"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="9dp"
        android:text="First Name:"
        app:layout_constraintEnd_toStartOf="@+id/et_v_details_fname"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="10dp"
        android:text="Last Name:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_v_details_lname"
        app:layout_constraintEnd_toStartOf="@+id/et_v_details_lname"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:text="Age:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_v_details_age"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:text="GPA:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_v_details_gpa"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="9dp"
        android:text="Major:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/et_v_details_major" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:text="Email:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_v_details_email"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/et_v_details_fname"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="29dp"
        android:ems="10"
        android:inputType="text"
        android:text="fname"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/tv_v_details_id" />

    <EditText
        android:id="@+id/et_v_details_lname"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="108dp"
        android:ems="10"
        android:inputType="text"
        android:text="lname"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_v_details_email"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="74dp"
        android:layout_marginBottom="68dp"
        android:ems="10"
        android:inputType="text"
        android:text="email"
        app:layout_constraintBottom_toBottomOf="@+id/et_v_details_age"
        app:layout_constraintEnd_toEndOf="@+id/et_v_details_age"
        app:layout_constraintStart_toStartOf="@+id/et_v_details_age"
        app:layout_constraintTop_toTopOf="@+id/et_v_details_lname" />

    <EditText
        android:id="@+id/et_v_details_age"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="247dp"
        android:ems="10"
        android:inputType="text"
        android:text="age"
        app:layout_constraintEnd_toEndOf="@+id/et_v_details_gpa"
        app:layout_constraintStart_toStartOf="@+id/et_v_details_gpa"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et_v_details_gpa"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginTop="53dp"
        android:layout_marginBottom="53dp"
        android:ems="10"
        android:inputType="text"
        android:text="GPA"
        app:layout_constraintBottom_toBottomOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="@+id/et_v_details_major"
        app:layout_constraintStart_toStartOf="@+id/et_v_details_major"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <EditText
        android:id="@+id/et_v_details_major"
        android:layout_width="0dp"
        android:layout_height="49dp"
        android:layout_marginBottom="293dp"
        android:ems="10"
        android:inputType="text"
        android:text="major"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/et_v_details_lname"
        app:layout_constraintStart_toStartOf="@+id/et_v_details_lname" />

    <Button
        android:id="@+id/btn_v_details_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="26dp"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_v_details_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="30dp"
        android:text="Update"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>